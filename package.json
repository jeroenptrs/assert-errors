{
  "name": "@jeroenpeeters/assert-errors",
  "version": "1.0.1",
  "private": false,
  "type": "module",
  "description": "A library attempting to make errors a bit more typed",
  "repository": "jeroenptrs/assert-errors",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "typescript"
  ],
  "author": {
    "name": "Jeroen Peeters",
    "email": "contact@jeroenpeeters.be"
  },
  "license": "ISC",
  "hooks": {
    "pre-commit": "pretty-quick --staged"
  },
  "scripts": {
    "build": "tsup && yarn lint:dist",
    "watch": "tsup --watch",
    "lint": "tsc --noEmit && eslint . --fix",
    "lint:dist": "eslint dist --fix \"dist/**.{cjs, js, ts}\"",
    "test": "jest",
    "test:code": "jest",
    "test:types": "tsd src",
    "prepare": "husky install"
  },
  "dependencies": {
    "tslib": "^2.6.1",
    "type-fest": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@jeroenpeeters/tsconfig": "^1.2.0",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "jest": {
    "verbose": true,
    "rootDir": "./src",
    "preset": "ts-jest",
    "collectCoverage": true,
    "coverageDirectory": "../coverage"
  }
}